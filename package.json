{
  "name": "nex-trust-sdk",
  "version": "1.0.0",
  "description": "SDK leve para verificação de identidade e análise de confiança",
  "main": "src/backend/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/backend/server.js",
    "dev": "nodemon src/backend/server.js",
    "dev:frontend": "live-server examples/vanilla --port=8080",
    "dev:backend": "nodemon src/backend/server.js",
    "test": "jest",
    "test:frontend": "jest tests/frontend",
    "test:backend": "jest tests/backend",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/ examples/",
    "lint:fix": "eslint src/ tests/ examples/ --fix",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "rollup -c rollup.config.js",
    "build:backend": "echo 'Backend build completed'",
    "clean": "rimraf dist/ coverage/",
    "docs": "jsdoc -c jsdoc.conf.json",
    "example:react": "cd examples/react && npm start",
    "example:vanilla": "live-server examples/vanilla --port=8080",
    "docker:build": "docker build -t nex-trust-sdk .",
    "docker:run": "docker run -p 3000:3000 nex-trust-sdk",
    "docker:dev": "docker-compose up --build"
  },
  "keywords": [
    "identity",
    "verification",
    "trust",
    "security",
    "fingerprinting",
    "behavioral-analysis",
    "fraud-detection",
    "sdk"
  ],
  "author": "NextTrust Team",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextrust/nex-trust-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/nextrust/nex-trust-sdk/issues"
  },
  "homepage": "https://github.com/nextrust/nex-trust-sdk#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "16"
          }
        }
      ]
    ]
  }
}
